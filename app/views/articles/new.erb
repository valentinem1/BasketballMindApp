<%= form_for @article do |f| %>
<%= f.label :title %>
<br>
<%= f.text_field :title %>
<br>
<%= f.label :content %>
<br>
<%= f.text_area :content %>
<br>
<%= f.label :user_id %>
<br>
<%= f.collection_select(:user_id, User.all, :id, :username) %>
<br>
<%= f.collection_select(:tag_id, Tag.all, :id, :team_name) %>
<br>
<%= f.submit %>
<% end %>